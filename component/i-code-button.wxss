/* component/i-code-button.wxss */
.container{
 display: flex;
 justify-content: center;
 flex: 1;
}
.box{
  border: 25rpx solid #FFE49B;
  width: 320rpx;
  height: 320rpx;
  background-color: #FEBE1B;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.container .box image{
  width: 180rpx;
  height: 180rpx;
}
.container .box text{
  width: 180rpx;
  text-align: center;
  color: #fff;
}