<view class="container">

  <view class="swiper-warp">
    <swiper indicator-dots circular interval="3000" autoplay>
      <swiper-item wx:for="{{bannerData}}" wx:key="key">
        <image src="{{item.banner_img}}"></image>
      </swiper-item>
    </swiper>
  </view>

  <view class="main">
    <i-code-button count="{{count}}" status="{{status}}" bind:getResult="getShopCode">
    </i-code-button>
  </view>
</view>>